{
  "private": true,
  "name": "oereb_client",
  "version": "2.0.1",
  "description": "OEREB Client based on Vue.js, OpenLayers and Bootstrap.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/gf-bl/oereb_client.git"
  },
  "keywords": [
    "oereb",
    "client",
    "react",
    "openlayers",
    "bootstrap",
    "web",
    "application"
  ],
  "author": "Karsten Deininger",
  "license": "BSD-2-Clause",
  "homepage": "https://gitlab.com/gf-bl/oereb_client#README",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.17.8",
    "@popperjs/core": "^2.11.4",
    "@reduxjs/toolkit": "^1.8.0",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/theming": "^6.4.19",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "css-loader": "^6.6.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.12.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "i18next": "^21.6.12",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-http-backend": "^1.4.0",
    "i18next-scanner": "^3.1.0",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-fetch-mock": "^3.0.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.5.3",
    "ol": "^6.13.0",
    "proj4": "^2.7.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.2",
    "react-redux": "^7.2.6",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.49.10",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "trim": ">=1.0.1",
    "ts-jest": "^27.1.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "oereb_client/static/src/**/*.{js,jsx}",
      "!oereb_client/static/src/**/*.stories.{js,jsx}",
      "!test/js/**/*.test.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "text",
      "text-summary"
    ],
    "coverageDirectory": "./.jest-coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/oereb_client/static/src/oereb_client.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ol)/)"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "./test/js/setupTests.js"
    ]
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
