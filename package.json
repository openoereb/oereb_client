{
  "private": true,
  "name": "oereb_client",
  "version": "2.0.0",
  "description": "OEREB Client based on Vue.js, OpenLayers and Bootstrap.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/gf-bl/oereb_client.git"
  },
  "keywords": [
    "oereb",
    "client",
    "react",
    "openlayers",
    "bootstrap",
    "web",
    "application"
  ],
  "author": "Karsten Deininger",
  "license": "BSD-2-Clause",
  "homepage": "https://gitlab.com/gf-bl/oereb_client#README",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.14.5",
    "@popperjs/core": "^2.11.0",
    "@reduxjs/toolkit": "^1.6.0",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "bootstrap": "^5.0.1",
    "bootstrap-icons": "^1.7.1",
    "codecov": "^3.8.3",
    "css-loader": "^5.2.7",
    "eslint": "^7.28.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "i18next": "^21.2.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-scanner": "^3.1.0",
    "i18next-xhr-backend": "^3.2.2",
    "jest": "^27.4.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "ol": "^6.8.1",
    "proj4": "^2.7.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-redux": "^7.2.6",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.7.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "oereb_client/static/src/**/*.{js,jsx}",
      "!test/js/**/*.test.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "text",
      "text-summary"
    ],
    "coverageDirectory": "./.jest-coverage/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
