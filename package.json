{
  "private": true,
  "name": "oereb_client",
  "version": "2.1.0",
  "description": "OEREB Client based on Vue.js, OpenLayers and Bootstrap.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/gf-bl/oereb_client.git"
  },
  "keywords": [
    "oereb",
    "client",
    "react",
    "openlayers",
    "bootstrap",
    "web",
    "application"
  ],
  "author": "Karsten Deininger",
  "license": "BSD-2-Clause",
  "homepage": "https://gitlab.com/gf-bl/oereb_client#README",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/runtime": "^7.23.2",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^1.9.7",
    "@storybook/addon-docs": "^7.5.2",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/addon-mdx-gfm": "^7.5.2",
    "@storybook/addons": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/react-webpack5": "^7.5.2",
    "@storybook/theming": "^7.5.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "acorn": "^8.10.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "glob-parent": "^6.0.2",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "i18next-scanner": "^4.4.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "ol": "^8.1.0",
    "proj4": "^2.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-redux": "^8.1.3",
    "remark-gfm": "^3.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "storybook": "^7.5.2",
    "storybook-react-i18next": "^2.0.9",
    "style-loader": "^3.3.3",
    "trim": ">=1.0.1",
    "trim-newlines": "^5.0.0",
    "ts-jest": "^29.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "oereb_client/static/src/**/*.{js,jsx}",
      "!oereb_client/static/src/**/*.stories.{js,jsx}",
      "!test/js/**/*.test.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "text",
      "text-summary"
    ],
    "coverageDirectory": "./.jest-coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/oereb_client/static/src/oereb_client.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ol)/)"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "./test/js/setupTests.js"
    ]
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  }
}
